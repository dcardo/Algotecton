// changes the canvas size (default 100*100)
size(400, 300);

// changes the background color
background(200);    

// sets the stroke color
stroke(0);

// sets the fill color
fill(255);

// draws a shape
ellipse(50,50,30,30);

// changes fill color
// the three values are for Red, Green and Blue (RGB)
fill(30, 0, 100); 

// draws another shape
rect(20, 100, 50, 50);

// changes fill color
fill(200, 0, 30);

//draws another shape
triangle(100, 200, 250, 100, 300, 40); 

